    "noImplicitAny": true,
    "sourceMap": true,
    "lib": [
      "es2016",
      "dom"
  ],
    "module": "es6",
    "target": "es5",
    "moduleResolution": "node"
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}